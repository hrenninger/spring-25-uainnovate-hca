@model List<HL7WebInterface.Models.HL7Message>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Timestamp</th>
            <th>Message Type</th>
            <th>Control ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var message in Model)
        {
            <tr>
                <td>@message.Id</td>
                <td>@message.DateTime.ToString("yyyy-MM-dd HH:mm")</td>
                <td>@message.MSH9</td>
                <td>@message.MSH10</td>
            </tr>
        }
    </tbody>
</table>
